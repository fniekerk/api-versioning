{
  "ApplicationName": "api-versioning",
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Grafana.Loki"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "GrafanaLoki",
        "Args": {
          "uri": "http://loki:3100",
          "labels": [
            {
              "key": "applogs",
              "value": "api-versioning"
            }
          ],
          "propertiesAsLabels": [
            "applogs"
          ]
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "ElasticConfiguration": {
    "Uri": "http://es-container:9200"
  },
  "GrafanaLokiConfiguration": {
    "Uri": "http://loki:3100"
  }
}
